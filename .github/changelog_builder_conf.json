{
  "categories": [
    {
      "title": "## New Features",
      "labels": ["release/feature-new"]
    },
    {
      "title": "## Changed Features",
      "labels": ["release/feature-changed"]
    },
    {
      "title": "## Fixes",
      "labels": ["release/bug"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "from tag ${{FROM_TAG}} to ${{TO_TAG}}\n\n${{CHANGELOG}}\n\ncategorized pr: ${{CATEGORIZED_COUNT}}\nuncategorized pr: ${{UNCATEGORIZED_COUNT}}\nignored pr: ${{IGNORED_COUNT}}\n<details>\n<summary>Total PR</summary>\n\n${{RELEASE_DIFF}}\n</details>",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "empty_template": "- no changes",
  "duplicate_filter": {
    "pattern": "\\[ABC-....\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 1000,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "api-(.+)",
      "flags": "gu"
    }
  },
  "base_branches": [
    "main"
  ]
}